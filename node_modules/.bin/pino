#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/camix/Desktop/poc-fastify-vs-express/node_modules/pino/node_modules:/mnt/c/Users/camix/Desktop/poc-fastify-vs-express/node_modules:/mnt/c/Users/camix/Desktop/node_modules:/mnt/c/Users/camix/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/camix/Desktop/poc-fastify-vs-express/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/camix/Desktop/poc-fastify-vs-express/node_modules/pino/node_modules:/mnt/c/Users/camix/Desktop/poc-fastify-vs-express/node_modules:/mnt/c/Users/camix/Desktop/node_modules:/mnt/c/Users/camix/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/camix/Desktop/poc-fastify-vs-express/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pino/bin.js" "$@"
else
  exec node  "$basedir/../pino/bin.js" "$@"
fi
